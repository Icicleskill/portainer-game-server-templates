{
    "version": "2",
    "templates": [
      {
        "type": 1,
        "title": "LGSM Counter Strike 2",
        "name": "LGSM Counter Strike 2",
        "note": "Based on LinuxGSM docker image",
        "description": "TEST desc",
        "categories": ["Game Servers"],
        "platform": "linux",
        "network": "host",
        "logo": "https://raw.githubusercontent.com/Icicleskill/portainer-game-server-templates/refs/heads/main/icons/cs2.webp",
        "image": "gameservermanagers/gameserver:cs2",
        "env": [
          {
            "name": "PUID",
            "label": "PUID",
            "default": "1000",
            "description": "for UserID"
          },
          {
            "name": "PGID",
            "label": "PGID",
            "default": "1000",
            "description": "for GroupID"
          },
          {
            "name": "TZ",
            "label": "TZ",
            "default": "America/Chicago",
            "description": "Specify a timezone to use for example Europe/Amsterdam"
          }
        ],
        "volumes": [
          {
            "container": "/data",
            "bind": "/"
          }
        ],
        "restart_policy": "unless-stopped",
        "maintainer": " https://github.com/icicleskill"
      }
    ]
  }
